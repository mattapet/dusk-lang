extern func println(int: Int) -> Void;
extern func readln() -> Int;

func isOdd(n: Int) -> Int {
    if n > 0 {
        return isEven(n - 1);
    }
    return 0;
}

func isEven(n: Int) -> Int {
    if n > 0 {
        return isOdd(n - 1);
    }
    return 1;
}

func main_() {
    let n = readln();
    println(isEven(n));
    println(isOdd(n));
}

