let ARRAY_SIZE = 5;

// var arr2 = [[1, 2, 3], [4, 5, 6]];
var arr3: Int[5] = [1, 2, 3, 4, 5];


func multiply(arr: inout Int[ARRAY_SIZE], val: Int){
    for i in 0..ARRAY_SIZE {
        arr[i] = arr[i] * val;
    }
}

func printArr(arr: Int[ARRAY_SIZE]) {
    for i in 0..ARRAY_SIZE {
        println(arr[i]);
    }
}

func main() {
    multiply(&arr3, 10);
    printArr(arr3);
}
