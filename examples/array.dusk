var arr3: Int[5] = [1, 2, 3, 4, 5];

func multiply(arr: Int[5], val: Int) -> Int[5] {
    for i in 0..5 {
        arr[i] = arr[i] * val;
    }
    return arr;
}

func main() {
    let res = multiply(arr3, 2);
    for i in 0..5 {
        println(res[i]);
    }
}
